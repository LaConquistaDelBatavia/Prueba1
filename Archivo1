
import random
numero  = random.randint(1, 8000)
print (numero)
n1 = float(input("Introduce un número: ") )
n2 = float(input("Introduce otro número: ") )
Contador = 0

while Contador < 10:
    if n1 == n2:
        print ("el número es igual")
    
    if n1 > n2:
        print ("el primer número es mayor que el segundo")

    if n1 < n2:
        print ("el primer número es menor que el segundo")
    
    Contador = (Contador+1)
